<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#fdb933" />
            <corners
                android:bottomLeftRadius="4dp"
                android:topLeftRadius="4dp"></corners>
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:top="1dp">
        <shape>
            <corners
                android:bottomLeftRadius="4dp"
                android:topLeftRadius="4dp"></corners>
            <solid android:color="#fff8ea" />
        </shape>
    </item>
</layer-list>